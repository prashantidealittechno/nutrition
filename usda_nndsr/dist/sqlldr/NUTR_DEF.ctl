OPTIONS (DIRECT=TRUE, PARALLEL=TRUE)
LOAD DATA
    INFILE './data.processed/NUTR_DEF.txt.trimmed'
    APPEND
    INTO TABLE NUTR_DEF
    FIELDS TERMINATED BY '^'
    OPTIONALLY ENCLOSED BY '~'
    TRAILING NULLCOLS
    (Nutr_No CHAR, Units CHAR, Tagname CHAR, NutrDesc CHAR, Num_Dec CHAR, SR_Order INTEGER EXTERNAL)