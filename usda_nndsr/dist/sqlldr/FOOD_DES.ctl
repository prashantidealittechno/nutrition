OPTIONS (DIRECT=TRUE, PARALLEL=TRUE)
LOAD DATA
    INFILE './data.processed/FOOD_DES.txt.trimmed'
    APPEND
    INTO TABLE FOOD_DES
    FIELDS TERMINATED BY '^'
    OPTIONALLY ENCLOSED BY '~'
    TRAILING NULLCOLS
    (NDB_No CHAR, FdGrp_Cd CHAR, Long_Desc CHAR, Shrt_Desc CHAR, ComName CHAR, ManufacName CHAR, Survey CHAR, Ref_desc CHAR, Refuse INTEGER EXTERNAL, SciName CHAR, N_Factor DECIMAL EXTERNAL, Pro_Factor DECIMAL EXTERNAL, Fat_Factor DECIMAL EXTERNAL, CHO_Factor DECIMAL EXTERNAL)