OPTIONS (DIRECT=TRUE, PARALLEL=TRUE, SKIP=1)
LOAD DATA
    INFILE './data.processed/CONVERSION_FACTOR.csv.trimmed'
    APPEND
    INTO TABLE CONVERSION_FACTOR
    FIELDS TERMINATED BY ','
    OPTIONALLY ENCLOSED BY '"'
    TRAILING NULLCOLS
    (FoodID INTEGER EXTERNAL, MeasureID INTEGER EXTERNAL, ConversionFactorValue INTEGER EXTERNAL, ConvFactorDateOfEntry DATE "yyyy-mm-dd")