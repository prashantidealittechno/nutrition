OPTIONS (DIRECT=TRUE, PARALLEL=TRUE, SKIP=1)
LOAD DATA
    INFILE './data.processed/REFUSE_AMOUNT.csv.trimmed'
    APPEND
    INTO TABLE REFUSE_AMOUNT
    FIELDS TERMINATED BY ','
    OPTIONALLY ENCLOSED BY '"'
    TRAILING NULLCOLS
    (FoodID INTEGER EXTERNAL, RefuseID INTEGER EXTERNAL, RefuseAmount DECIMAL EXTERNAL, RefuseDateOfEntry DATE "yyyy-mm-dd")