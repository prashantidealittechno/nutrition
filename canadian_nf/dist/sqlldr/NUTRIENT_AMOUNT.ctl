OPTIONS (DIRECT=TRUE, PARALLEL=TRUE, SKIP=1)
LOAD DATA
    INFILE './data.processed/NUTRIENT_AMOUNT.csv.trimmed'
    APPEND
    INTO TABLE NUTRIENT_AMOUNT
    FIELDS TERMINATED BY ','
    OPTIONALLY ENCLOSED BY '"'
    TRAILING NULLCOLS
    (FoodID INTEGER EXTERNAL, NutrientID INTEGER EXTERNAL, NutrientValue DECIMAL EXTERNAL, StandardError DECIMAL EXTERNAL, NumberOfObservations INTEGER EXTERNAL, NutrientSourceID INTEGER EXTERNAL, NutrientDateOfEntry DATE "yyyy-mm-dd")