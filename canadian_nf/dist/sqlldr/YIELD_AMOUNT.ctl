OPTIONS (DIRECT=TRUE, PARALLEL=TRUE, SKIP=1)
LOAD DATA
    INFILE './data.processed/YIELD_AMOUNT.csv.trimmed'
    APPEND
    INTO TABLE YIELD_AMOUNT
    FIELDS TERMINATED BY ','
    OPTIONALLY ENCLOSED BY '"'
    TRAILING NULLCOLS
    (FoodID INTEGER EXTERNAL, YieldID INTEGER EXTERNAL, YieldAmount DECIMAL EXTERNAL, YieldDateOfEntry DATE "yyyy-mm-dd")